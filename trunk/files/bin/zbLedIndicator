#!/bin/sh

. /lib/functions/leds.sh

# example zbLedIndicator "wr8305rt:sys" "on"

status_led=$1

if [ $2 == "on" ]; then
	status_led_on
elif [ $2 == "off" ]; then
	status_led_off
elif [ $2 == "blink" ]; then
	status_led_blink_slow
else
	echo "no devices opterton"
	return 2
fi

