#!/bin/sh

if [ -e /sys/class/leds/zpower/brightness ]; then
	if [ $1 == "on" ]; then
		echo "zigbee Power On"
		echo 1 > /sys/class/leds/zpower/brightness
		return 1
	else	
		echo "zigbee Power Off"
		echo 0 > /sys/class/leds/zpower/brightness
		return 0
	fi
else
	echo "no found leds devices"
	return 2
fi


